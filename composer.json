{
  "name": "awesomemotive/wp-mail-smtp",
  "description": "Reconfigures the wp_mail() function to use SMTP instead of mail().",
  "type": "wordpress-plugin",
  "keywords": [
    "mail",
    "smtp",
    "wp_mail",
    "mailer",
    "phpmailer"
  ],
  "homepage": "https://wordpress.org/plugins/wp-mail-smtp/",
  "license": "GPLv3",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "composer/installers": "~1.0",
    "php": ">=5.3",
    "google/apiclient": "^2.2.1"
  },
  "config": {
    "prepend-autoloader": false
  },
  "autoload": {
    "exclude-from-classmap": [
      "**/test/",
      "**/tests/",
      "**/Test/",
      "**/Tests/",
      "/vendor/composer/installers/",
      "/vendor/monolog/",
      "/vendor/google/apiclient-services/src/Google/Service/Safebrowsing/",
      "/src/"
    ]
  }
}
